<%- include("header") -%>
    <div class="box" id ="heading">
      <h1 style="color: #4c857f"><%=listtitle%> </h1>
    </div>
    <div class="box">
      <% for(var i = 0; i<newlistitems.length; i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%= newlistitems[i] %> </p>
        </div>
      <% } %>
      <form class="" action="/" method="post">
        <input type="text" name="newitem" placeholder="Enter New Item" autocomplete="off">
        <button type="submit" name="list" value=<%=listtitle%> style="color: #4c857f">+</button>
      </form>
    </div>
  <%- include("footer") -%>
